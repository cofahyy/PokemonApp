<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:drawable="@drawable/grass_gradient_background" />

    <!-- Even More Dark Background Blades -->
    <item
